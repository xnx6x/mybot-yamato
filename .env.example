# WhatsApp Bot Configuration
# Copy this file to .env and fill in your values

# Groq AI API Key (Required for AI features)
# Get it from: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# HTTP Server Port (Optional, defaults to 3000)
PORT=3000

# Admin Number for restart command (Optional)
# Add your WhatsApp number without + or country code
ADMIN_NUMBER=your_phone_number_here

# Environment (Optional)
NODE_ENV=production

# Additional Configuration (Optional)
BOT_NAME=Yamato Bot
BOT_VERSION=3.0